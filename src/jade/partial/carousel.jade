#carousel-example-generic(data-ride="carousel").carousel.slide
	ol.carousel-indicators
		li(data-target="#carousel-example-generic" data-slide-to="0").active
		li(data-target="#carousel-example-generic" data-slide-to="1")
		li(data-target="#carousel-example-generic" data-slide-to="2")
		li(data-target="#carousel-example-generic" data-slide-to="3")
	.carousel-inner(role="listbox")
		.item.active
			img(src="images/1.jpg", alt="a", width="100%", height=1080)
			.carousel-caption Tło jednej z moich stron.
		.item
			img(src="images/2.jpg", alt="b", width="100%", height=1080)
			.carousel-caption Ładne zdjęcie.
		.item
			img(src="images/3.jpg", alt="c", width="100%", height=1080)
			.carousel-caption Tajemniczy pokemon.
		.item
			img(src="images/4.jpg", alt="d", width="100%", height=1080)
			.carousel-caption Witalizm.
	a(href="#carousel-example-generic", role="button", data-slide="prev").left.carousel-control
		span.glyphicon.glyphicon-chevron-left(aria-hidden="true")
		span.sr-only Previous
	a(href="#carousel-example-generic", role="button", data-slide="next").right.carousel-control
		span.glyphicon.glyphicon-chevron-right(aria-hidden="true")
		span.sr-only Next